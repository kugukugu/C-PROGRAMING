#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main()
{
	int n;
	printf("정수를 입력하시오.");
	scanf("%d", &n);

	int i,k;
	int max = 0;
	int min = 0;
	for (i=0; i<n; i++)
	{
		scanf("%d", &k);

		if ( k > max || max==k )
		{
			max = k;
		}

		if (k < min || min == k)
		{
			min = k;
		}

	}

	printf("최댓값은 %d이고 최솟값은 %d입니다.\n", max, min);

	return 0;
}
