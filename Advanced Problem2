#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int h, n,m ;
    scanf("%d %d %d", &h,&n,&m);
    
    int k=0;
    int j=0;
    
    while(1)
    {
        k += n;
        j++;
        
        if(k >= h)
        {
            break;
        }
        
    }
    printf("안 미끄러지면 %d\n", j);
    
    k = 0;
    j = 0;
    
    while(1)
    {
        k += n;
        
        j++;
        
        if(k >= h)
        {
            break;
        }
        
        k -= m;
    }
    printf("미끄러지면 %d\n", j);
    
    return 0;
}
